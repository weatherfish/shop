
<swiper indicator-dots="{{indicatorDots}}" 
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{ circular }}" bindchange="swiperchange">
  <block wx:for="{{imgUrls}}" wx:key="images">
    <swiper-item>
      <image  src="{{item}}" class="slide-image"/>
    </swiper-item>
  </block>
</swiper>